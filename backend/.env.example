# Example environment for vod-tender (copy to backend/.env)

# Twitch credentials (required to enable chat recording)
TWITCH_CHANNEL=
TWITCH_BOT_USERNAME=
TWITCH_OAUTH_TOKEN=

# VOD context
TWITCH_VOD_ID=demo-vod-id
# RFC3339, e.g. 2025-01-01T00:00:00Z
TWITCH_VOD_START=

# Database
DB_DSN=vodtender.db
